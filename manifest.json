{
  "name": "Douban Album Helper",
  "description": "Play music in album page",
  "version": "1.2.2",

  "permissions": [ "http://*/dial/track/*", "http://*.douban.com/*", "https://*.douban.com/*", "http://music.163.com/*" ],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "content_scripts" : [
    {
      "matches" : [
        "http://*.music.163.com/*"
      ],
      "js" : ["jquery.min.js" , "content.js"]
    }
  ],

  "manifest_version": 2
}
